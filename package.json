{
    "name": "first-learn-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "open-browser": "start http://localhost:3000",
        "dev": "npm run open-browser && next dev",
        "start:dev": "concurrently \" npm run dev\" \"npm run server\"",
        "build": "next build",
        "build-api": "concurrently \"npm run server\" \"npm run build\"",
        "start": "next start",
        "lint": "next lint",
        "server": "json-server --watch ./json-server/db.json --port 8000"
    },
    "dependencies": {
        "@next/font": "13.1.2",
        "concurrently": "^7.6.0",
        "eslint": "8.32.0",
        "eslint-config-next": "13.1.2",
        "json-server": "^0.17.1",
        "next": "13.1.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-icons": "^4.7.1"
    }
}
